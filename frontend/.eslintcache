Cancel}\n      >\n        <Row>\n          <Col span={6}>{uploadButton}</Col>\n          <Col span={16} style={{ padding: \"5px\" }}>\n            <p>Select a new {data[0]}.</p>\n            <p>\n              Please select a photo that is <br></br>\n              less than 2mb and a jpeg/png.\n            </p>\n          </Col>\n        </Row>\n      </Modal>\n    </>\n  );\n}\n\nexport default UploadPhoto;\n","/home/anguilar/cloudsound/frontend/src/components/VideoHeader/index.js",[],"/home/anguilar/cloudsound/frontend/src/store/profile.js",["121","122"],{"ruleId":"123","severity":1,"message":"124","line":10,"column":10,"nodeType":"125","messageId":"126","endLine":10,"endColumn":24},{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","replacedBy":"130"},{"ruleId":"123","severity":1,"message":"131","line":9,"column":13,"nodeType":"125","messageId":"126","endLine":9,"endColumn":34},{"ruleId":"123","severity":1,"message":"132","line":2,"column":10,"nodeType":"125","messageId":"126","endLine":2,"endColumn":18},{"ruleId":"123","severity":1,"message":"133","line":2,"column":20,"nodeType":"125","messageId":"126","endLine":2,"endColumn":28},{"ruleId":"123","severity":1,"message":"134","line":53,"column":7,"nodeType":"125","messageId":"126","endLine":53,"endColumn":15},{"ruleId":"123","severity":1,"message":"135","line":6,"column":10,"nodeType":"125","messageId":"126","endLine":6,"endColumn":22},{"ruleId":"123","severity":1,"message":"136","line":7,"column":10,"nodeType":"125","messageId":"126","endLine":7,"endColumn":17},{"ruleId":"123","severity":1,"message":"137","line":16,"column":3,"nodeType":"125","messageId":"126","endLine":16,"endColumn":13},{"ruleId":"123","severity":1,"message":"138","line":23,"column":15,"nodeType":"125","messageId":"126","endLine":23,"endColumn":19},{"ruleId":"123","severity":1,"message":"139","line":24,"column":9,"nodeType":"125","messageId":"126","endLine":24,"endColumn":15},{"ruleId":"123","severity":1,"message":"140","line":37,"column":9,"n